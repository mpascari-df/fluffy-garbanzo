{
  "displayName": "MongoDB Pipeline - Data Insights",
  "gridLayout": {
    "widgets": [
      {
        "title": "ðŸ“š Events by Collection",
        "xyChart": {
          "dataSets": [{
            "timeSeriesQuery": {
              "timeSeriesFilter": {
                "filter": "metric.type=\"logging.googleapis.com/user/events_by_collection\"",
                "aggregation": {
                  "alignmentPeriod": "300s",
                  "perSeriesAligner": "ALIGN_RATE"
                }
              }
            },
            "plotType": "STACKED_AREA"
          }]
        }
      },
      {
        "title": "ðŸ“ˆ Hourly Event Volume",
        "xyChart": {
          "dataSets": [{
            "timeSeriesQuery": {
              "timeSeriesFilter": {
                "filter": "metric.type=\"logging.googleapis.com/user/change_stream_events\"",
                "aggregation": {
                  "alignmentPeriod": "3600s",
                  "perSeriesAligner": "ALIGN_DELTA"
                }
              }
            },
            "plotType": "STACKED_BAR"
          }]
        }
      },
      {
        "title": "ðŸ”„ Pub/Sub Success Rate",
